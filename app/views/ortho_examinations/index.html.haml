- @page_title = "Ortho Examination List"
.container
  .header
    .left.subtitle
      %h1 Ortho Examination
      %span
        %i.icon-list.icons-black
        List of Ortho Examination Records
    = link_to raw('<i class="icon-plus-sign icons-black"></i>&nbsp;&nbsp;Add Record'), "#AddOrthoExam", "role" => "button", "data-toggle" => "modal", :class => 'btn btn-success right'
  .content.both
    .row
      .span12
        #posts_list
          = render(:partial => "list")
#AddOrthoExam{'class' => 'modal hide fade record modal-modal-exam'}
  = render(:partial => "form")

#EditOrthoExam{'class' => 'modal hide fade record modal-modal-exam'}
  = render(:partial => "edit_form")

