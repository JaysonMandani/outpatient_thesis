<script type="text/javascript" charset="utf-8">
  $(function() {
    new Highcharts.Chart({
      chart: {
        renderTo: "ortho_chart"
      },
      title: {
        text: "Orthodontics"
      },
      xAxis: {
        type: "datetime"
      },
      yAxis: {
        title: {
          text: "Outpatient"
        }
      },
      series: [{
        name: "Summary",
        pointInterval: <%= 1.day * 1000 %>,
        pointStart: <%= 3.weeks.ago.to_i * 1000 %>,
        data: [2, 6, 8, 5, 6, 9, 5, 6, 3, 4, 3]
      },
      {
        name: "Scheduled",
        pointInterval: <%= 1.day * 1000 %>,
        pointStart: <%= 3.weeks.ago.to_i * 1000 %>,
        data: [1, 4, 6, 8, 3, 4, 9, 6, 8, 3, 6]
      }]
    });
  });
  $(function() {
    new Highcharts.Chart({
      chart: {
        renderTo: "pedia_chart"
      },
      title: {
        text: "Pediatrics"
      },
      xAxis: {
        type: "datetime"
      },
      yAxis: {
        title: {
          text: "Outpatient"
        }
      },
      series: [{
        name: "Summary",
        pointInterval: <%= 1.day * 1000 %>,
        pointStart: <%= 3.weeks.ago.to_i * 1000 %>,
        data: [2, 6, 8, 5, 6, 9, 5, 6, 3, 4, 3]
      },
      {
        name: "Scheduled",
        pointInterval: <%= 1.day * 1000 %>,
        pointStart: <%= 3.weeks.ago.to_i * 1000 %>,
        data: [3 ,5 ,9 ,2 ,7 ,4 ,8 ,2 ,3 ,7 ,4]
      }]
    });
  });
</script>
  <div class="side-content grid_20 prefix_2 suffix_2">
  
    <div class="content-module">
  
      <div class="content-module-heading cf">
        <h3> Analytics </h3>
      </div> <!-- end of content-module-heading -->
  
      <div class="content-module-main cf">
        
       <div id="ortho_chart" style="width:750px; height:300px;"></div>

       <div id="pedia_chart" style="width:750px; height:300px;"></div>
  
      </div> <!-- end of content-module-main -->
  
    </div> <!-- end of content-module -->
  
  </div> <!-- end of side-content -->
  

  


